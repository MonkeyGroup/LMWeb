@using LM.Model.Model
@using LM.Service.Security
@using LM.Utility
@{
    var config = CurrentContext.Get("HomePageConfig") as HomePageConfigModel ?? new HomePageConfigModel();
}
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <title>@ViewBag.Title - @(string.IsNullOrEmpty(config.SiteName) ? "中国电子化工新材料产业联盟" : config.SiteName)</title>
    <link href="@(string.IsNullOrEmpty(config.LogoSrc)?"/favicon.ico":config.LogoSrc)" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width"/>
    
    @Html.RenderCss("/Content/css/index.css")
    @*@Html.RenderCss("/Content/css/tb.css")*@
    @*@Html.RenderCss("/Content/css/tpqh.css")*@
    @*@Html.RenderCss("/Content/css/style.css")*@
    @*@Html.RenderCss("/Content/css/nav.css")*@ 

    @Html.RenderScript("/Content/js/jquery-1.9.1.js")
    @Html.RenderScript("/Content/js/homePage.js")
    @*@Html.RenderScript("/Content/js/logoRoll.js")*@
    @Html.RenderScript("/Content/js/tab.js")
    @Html.RenderScript("/Content/js/slide.js")
    @Html.RenderScript("/Content/js/forTable.js")

    @RenderSection("partialScriptCss", false)

</head>
<body>
    <header>
        @{
            Html.RenderAction("Header", "Common");
        }
    </header>
    <div id="">
        @RenderBody()
    </div>
    <footer>
        @{
            Html.RenderAction("Footer", "Common");
        }
    </footer>

    @RenderSection("endScripts", false)
</body>
</html>
